ngx_addon_name=ngx_http_auth_dav_module

if test -n "$ngx_module_link"; then
    ngx_module_type=HTTP
    ngx_module_name=ngx_http_auth_dav_module
    ngx_module_incs=
    ngx_module_deps=
    ngx_module_srcs="$ngx_addon_dir/ngx_http_auth_dav_module.c"
    ngx_module_libs="-ldav_cloud -lmysqlclient -L/usr/lib64/mysql"

    . auto/module
else
    HTTP_MODULES="$HTTP_MODULES ngx_http_auth_dav_module"
    NGX_ADDON_SRCS="$NGX_ADDON_SRCS $ngx_addon_dir/ngx_http_auth_dav_module.c"
    CORE_LIBS="$CORE_LIBS -ldav_cloud -lmysqlclient -L/usr/lib64/mysql"
fi